{
    "name": "mkb/katm-sdk-laravel",
    "description": "Katm SDK for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "katm",
        "sdk",
        "laravel"
    ],
    "version": "0.1.0",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "spatie/laravel-data": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Mkb\\KatmSdkLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mkb\\KatmSdkLaravel\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mkb\\KatmSdkLaravel\\Providers\\KatmSdkServiceProvider"
            ],
            "aliases": {
                "Katm": "Mkb\\KatmSdkLaravel\\Facades\\Katm"
            }
        }
    },
    "authors": [
        {
            "name": "Diyorbek",
            "email": "diordev9@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^10.6"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
